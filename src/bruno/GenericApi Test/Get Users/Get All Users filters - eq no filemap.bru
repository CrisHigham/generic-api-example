meta {
  name: Get All Users filters - eq no filemap
  type: http
  seq: 8
}

get {
  url: {{secure}}{{server}}{{base}}/test-user?limit=100&lastName=eq:Kent
  body: json
  auth: bearer
}

params:query {
  limit: 100
  lastName: eq:Kent
}

auth:bearer {
  token: {{token}}
}

tests {
  let testCode = 200;
  const responseJson = res.getBody();
  
  test("Get All Users - filters - eq no filemap", function(){
      expect(res.getStatus()).to.equal(testCode)
      expect(responseJson.data[0].firstName).to.eql("Clark");
  })
  
  if (res.getStatus() != testCode){
      console.log(responseJson);
      bru.runner.stopExecution();
  }
}
